<?xml version='1.0' encoding='utf-8'?>
<widget id="com.bayteq.fibeca" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fibeca</name>
   
    <author href="http://www.bayteq.com" email="info@bayteq.com">
        Bayteq
    </author>

    <icon src="icon.png" />
    <splash src="splash.png" />

    <content src="index.html" />
    
    <plugin name="cordova-plugin-whitelist" spec="n/a" />
    <plugin name="cordova-plugin-geolocation" spec="n/a" />

    <access origin="*" />
    
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

    <preference name="orientation" value="portrait" />

</widget>
